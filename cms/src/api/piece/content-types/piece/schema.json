{
  "kind": "collectionType",
  "collectionName": "pieces",
  "info": {
    "singularName": "piece",
    "pluralName": "pieces",
    "displayName": "Piece"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Price": {
      "type": "decimal"
    },
    "Stock": {
      "type": "integer"
    },
    "Category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "pieces"
    },
    "marque": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::marque.marque",
      "inversedBy": "pieces"
    },
    "Photos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
