{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "marques": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::marque.marque",
      "inversedBy": "categories"
    },
    "pieces": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::piece.piece",
      "mappedBy": "Category"
    }
  }
}
